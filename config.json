{
    // The window that the program looks for
    "window_name": "EscapeFromTarkov",
    // Settings related to auto-accept
    "auto_accept": {
        // determines if this functionality is enabled
        "enabled": false,
        // List of names of players you wish to auto-accept invites from. Syntax: ["Name 1", "Name 2"]
        // If auto-accept is enabled and list is empty, all invites will be accepted
        "allowed_names": [],
        // The area of the screen that the screenshot targets, in percentage.
        "bounding_box": {
            "left": 0.35,
            "right": 0.35,
            "top": 0.42,
            "bottom": 0.42
        }
    },
    // settings related to deployment warning
    "deploy_warning": {
        // determines if this functionality is enabled
        "enabled": true,
        // If true, playus sound every 2 seconds up to 5 remaining, then every 1 seconds until 0
        // If false, plays sound every second
        "staggered": true,
        // The area of the screen that the screenshot targets, in percentage
        "bounding_box": {
            "left": 0.41,
            "right": 0.41,
            "top": 0.45,
            "bottom": 0.2
        }
    }
}
